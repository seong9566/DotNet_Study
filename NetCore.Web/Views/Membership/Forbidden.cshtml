@{
    ViewData["Title"] = "접근 거부";
    Layout = "_Layout";
}

<h2>@ViewData["Title"]</h2>

<div class="alert alert-warning" role="alert">
    @Html.Raw(ViewData["Message"] ?? "해당 페이지에 접근할 수 없습니다.")
</div>

<p class="mt-3">
    <a class="btn btn-primary" asp-controller="Membership" asp-action="Index">멤버십 홈으로</a>
    <a class="btn btn-secondary ms-2" asp-controller="Home" asp-action="Index">홈으로</a>
</p>
